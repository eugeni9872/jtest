pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                sh 'echo Build Step'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                sh 'echo Test Step'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                sh 'echo Deploy Step'
            }
        }
    }
}